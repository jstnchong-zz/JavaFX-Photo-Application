<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="548.0" prefWidth="613.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AlbumViewController">
   <children>
      <Button fx:id="logoutButton" layoutX="525.0" layoutY="507.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      <Button fx:id="addButton" layoutX="34.0" layoutY="406.0" mnemonicParsing="false" onAction="#addPhotoButton" text="Add" />
      <Button fx:id="viewButton" layoutX="283.0" layoutY="406.0" mnemonicParsing="false" onAction="#viewPhotoButton" text="View" />
      <Button fx:id="deleteButton" layoutX="526.0" layoutY="406.0" mnemonicParsing="false" onAction="#deletePhotoButton" text="Delete" />
      <Button fx:id="moveButton" layoutX="293.0" layoutY="494.0" mnemonicParsing="false" onAction="#movePhoto" text="Move" />
      <ListView fx:id="photoList" layoutX="25.0" layoutY="76.0" prefHeight="319.0" prefWidth="563.0" />
      <Label fx:id="albumLabel" alignment="CENTER" layoutX="49.0" layoutY="6.0" prefHeight="65.0" prefWidth="514.0">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
      <Button fx:id="captionButton" layoutX="522.0" layoutY="455.0" mnemonicParsing="false" onAction="#captionPhotoButton" text="Caption" />
      <Label layoutX="7.0" layoutY="460.0" text="Caption Here:" />
      <TextField fx:id="captionTextField" layoutX="99.0" layoutY="455.0" prefHeight="27.0" prefWidth="414.0" />
      <Button fx:id="copyButton" layoutX="352.0" layoutY="494.0" mnemonicParsing="false" onAction="#copyPhoto" text="Copy" />
      <Label layoutX="13.0" layoutY="499.0" text="Album Name:" />
      <TextField fx:id="albumTextField" layoutX="107.0" layoutY="494.0" />
   </children>
</AnchorPane>
